<!DOCTYPE html>
<html>
  <head>
    <title>Heka</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <link href="/assets/bootstrap.min.css" rel="stylesheet">
    <!-- Custom styles for this template -->
    <link href="/assets/modern-business.css" rel="stylesheet">
   <link href="/assets/style.css" rel="stylesheet">
    <link href="/assets/media-query.css" rel="stylesheet">
    <link href="/assets/fa-brands.css" rel="stylesheet">
    <link href="/assets/fa-brands.min.css" rel="stylesheet">
    <link href="/assets/fa-regular.css" rel="stylesheet">
    <link href="/assets/fa-regular.min.css" rel="stylesheet">
    <link href="/assets/fa-solid.css" rel="stylesheet">
    <link href="/assets/fa-solid.min.css" rel="stylesheet">
    <link href="/assets/fontawesome.css" rel="stylesheet">
    <link href="/assets/fontawesome.min.css" rel="stylesheet">
    <link href="/assets/fontawesome-all.css" rel="stylesheet">
    <link href="/assets/fontawesome-all.min.css" rel="stylesheet">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
     <link href="/assets/select2.min.css" rel="stylesheet" />
    <script src="/assets/select2.min.js"></script>
    
    <link href="/assets/cryptocoins.css" rel="stylesheet">
  <script>
        $(document).ready(function(){
            $('.form-group-change').hide()
            $(".uppercase").click(function(){
              $(".form-group-change").toggle();
            });
            $(document).mouseup(function (e) {
                 var currency_list = $(".form-group-change");
                 if (!$('#open').is(e.target) && !currency_list.is(e.target) && currency_list.has(e.target).length == 0) {
                    currency_list.hide(500);
                 }
             });
       
      $('.form-group-change1').hide()
            $(".uppercase1").click(function(){
              $(".form-group-change1").toggle();
            });
            $(document).mouseup(function (e) {
                 var currency_list = $(".form-group-change1");
                 if (!$('#open').is(e.target) && !currency_list.is(e.target) && currency_list.has(e.target).length == 0) {
                     currency_list.hide(500);
                 }
             });
        });
    </script>

    <%= csrf_meta_tags %>
    <%= alertify_flash(wait: 20) %>
  </head>
  <body>
    <%= render 'common/header' %>
    <%= yield %>
    <% if !((params[:controller]=="devise/sessions") || (params[:controller]== "devise/passwords") || (params[:controller]== "devise/registrations") ) %>
      <%= render 'common/footer' %>
    <% end %>
  </body>
</html>